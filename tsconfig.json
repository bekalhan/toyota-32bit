{
    "compilerOptions": {
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "useUnknownInCatchVariables": false,
      "lib": [
        "esnext",
        "es2018",
        "DOM"
      ],
      "noImplicitAny": false,
      "baseUrl": "src",
      "esModuleInterop": true,
      "target": "es5",
      "allowJs": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
      "incremental": true,
      "paths": {
        "react": [ "./node_modules/@types/react" ]
      },
      "allowSyntheticDefaultImports" : true

    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/**/*.ts",
      "**/**/*.tsx"
, "src/App.js", "src/pages/entrance/Login.jsx"    ],
    "exclude": [
      "node_modules"
    ]
  }